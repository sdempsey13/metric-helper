{
  "name": "Metric Cheat Sheet",
  "description": "Easily accesible dropdown metric prefixes cheat sheet",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "images/icon128.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+M"
      },
    "description": "Opens popup.html"
    }
  }
}
